{
  "name": "solana-paper-trader-extension",
  "version": "0.1.0",
  "description": "Paper trading overlay for Solana spot on Axiom and Terminal (Padre)",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "esbuild src/content.source.js --bundle --outfile=src/content.bundle.js --format=iife",
    "build:secure": "esbuild src/content.source.js --bundle --minify --outfile=src/content.bundle.js --format=iife",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,css,html}\""
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0"
  },
  "keywords": [
    "chrome-extension",
    "solana",
    "paper-trading",
    "trading",
    "cryptocurrency"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": ""
  }
}